{
  "name": "erajs-frontend",
  "version": "0.2.0",
  "main": "dist/index.js",
  "repository": "https://github.com/miswanting/Era.js.git",
  "author": "Miswanting",
  "license": "GPL-3.0+",
  "scripts": {
    "setup": "yarn",
    "standardize": "standard --fix",
    "package-dev": "webpack --config webpack.config.dev.js",
    "package-prod": "webpack --config webpack.config.prod.js",
    "debug": "chcp 65001 && electron .",
    "P-D": "call yarn run package-dev && call yarn run debug",
    "build": "electron-builder --dir"
  },
  "devDependencies": {
    "@popperjs/core": "latest",
    "@vue/compiler-sfc": "latest",
    "animate.css": "latest",
    "animejs": "latest",
    "clean-webpack-plugin": "latest",
    "css-loader": "latest",
    "csshake": "latest",
    "d3-geo-voronoi": "latest",
    "electron": "latest",
    "electron-builder": "latest",
    "electron-devtools-installer": "latest",
    "electron-is-dev": "latest",
    "gl-matrix": "latest",
    "html-webpack-plugin": "latest",
    "lodash": "latest",
    "normalize.css": "latest",
    "pug": "latest",
    "pug-plain-loader": "latest",
    "simplex-noise": "latest",
    "standard": "latest",
    "style-loader": "latest",
    "stylus": "latest",
    "stylus-loader": "latest",
    "three": "latest",
    "tippy.js": "latest",
    "vue": "next",
    "vue-loader": "next",
    "vue-router": "next",
    "vue-style-loader": "latest",
    "vuex": "next",
    "webpack": "latest",
    "webpack-cli": "latest",
    "worker-loader": "latest",
    "worker-plugin": "latest"
  },
  "build": {
    "appId": "net.erajs.frontend",
    "productName": "Era.js",
    "directories": {
      "output": "build"
    }
  },
  "standard": {
    "ignore": [
      "/dist/"
    ],
    "global": [
      "onmessage",
      "postMessage"
    ]
  }
}